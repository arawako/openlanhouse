<?xml version="1.0"?>
<gconfschemafile><schemalist>
    
    <schema>
      <key>/schemas/apps/openlh-client/ui/visible</key>
      <applyto>/apps/openlh-client/ui/visible</applyto>
      <owner>openlh-client</owner>
      <type>bool</type>
      <default>true</default>
      
      <locale name="C">
        <short>Whether the window is visible</short>
        <long>When set to false, starts only as tray icon with hidden window.</long>
      </locale>
      
    </schema>
    
    <schema>
      <key>/schemas/apps/openlh-client/port</key>
      <applyto>/apps/openlh-client/port</applyto>
      <owner>openlh-client</owner>
      <type>int</type>
      <default>4558</default>
      
      <locale name="C">
        <short>Port number</short>
        <long>Port number used to connect into server.</long>
      </locale>
      
    </schema>
    
    <schema>
      <key>/schemas/apps/openlh-client/server</key>
      <applyto>/apps/openlh-client/server</applyto>
      <owner>openlh-client</owner>
      <type>string</type>
      
      <locale name="C">
        <short>>Server host name</short>
        <long>Server host name.</long>
      </locale>
      
    </schema>
    
    <schema>
      <key>/schemas/apps/openlh-client/hash_id</key>
      <applyto>/apps/openlh-client/hash_id</applyto>
      <owner>openlh-client</owner>
      <type>string</type>
      
      <locale name="C">
        <short>Hash code used for machine identification in server</short>
        <long>Hash code used for machine identification in server.</long>
      </locale>
      
    </schema>
    
    <schema>
      <key>/schemas/apps/openlh-client-applet/per-applet/show_username</key>
      <owner>openlh-client-applet</owner>
      <type>bool</type>
      <default>False</default>
      <locale name="C">
        <short>Show Username</short>
        <long>Show Username.</long>
      </locale>
    </schema>
    
    <schema>
      <key>/schemas/apps/openlh-client-applet/per-applet/show_credit</key>
      <owner>openlh-client-applet</owner>
      <type>bool</type>
      <default>True</default>
      <locale name="C">
        <short>Show User credit</short>
        <long>Show User credit.</long>
      </locale>
    </schema>
    
    <schema>
      <key>/schemas/apps/openlh-client-applet/per-applet/show_time</key>
      <owner>openlh-client-applet</owner>
      <type>bool</type>
      <default>False</default>
      <locale name="C">
        <short>Show Time</short>
        <long>Show Time.</long>
      </locale>
    </schema>
    
    <schema>
      <key>/schemas/apps/openlh-client-applet/per-applet/show_time_elapsed</key>
      <owner>openlh-client-applet</owner>
      <type>bool</type>
      <default>False</default>
      <locale name="C">
        <short>Show Time Elapsed</short>
        <long>Show Time Elapsed.</long>
      </locale>
    </schema>
    
    <schema>
      <key>/schemas/apps/openlh-client-applet/per-applet/show_time_left</key>
      <owner>openlh-client-applet</owner>
      <type>bool</type>
      <default>True</default>
      <locale name="C">
        <short>Show Time Remaining</short>
        <long>Show Time Remaining.</long>
      </locale>
    </schema>
    
    <schema>
      <key>/schemas/apps/openlh-client-applet/per-applet/show_total_to_pay</key>
      <owner>openlh-client-applet</owner>
      <type>bool</type>
      <default>True</default>
      <locale name="C">
        <short>Show Total to pay</short>
        <long>Show Total to pay.</long>
      </locale>
    </schema>
    
    <schema>
      <key>/schemas/apps/openlh-client-applet/per-applet/show_titles</key>
      <owner>openlh-client-applet</owner>
      <type>bool</type>
      <default>True</default>
      <locale name="C">
        <short>Show Titles</short>
        <long>Show Titles.</long>
      </locale>
    </schema>
    
</schemalist></gconfschemafile>
